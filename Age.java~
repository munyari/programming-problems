/*
 * Timus OJ - 1104
 * Don't ask a woman about her age
 */
import java.io.*;
import java.util.*;

public class Age {
  public static void main(String[] args) throws IOException {
    BufferedReader f = new BufferedReader(new InputStreamReader(System.in));
    String s = f.readLine();
    int num = toDecimal(s);
    int minBase = minBase(s); // min base possible, with the symbols used
    boolean found = false;
    for (int i = minBase; i <= 36; i++) {
       if (num % (i - 1) == 0) {
         found = true;
         System.out.println(i);
         break;
       }
    }
    if (!found)
       System.out.println(-1);
    f.close();
    System.exit(0);
  }
  private static int minBase(String s) {
    String bases = "0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ";
    int max = 2;
    for (int i = 0; i < s.length(); i++) {
      max = Math.max(max, bases.indexOf(s.charAt(i)));
    }
    return max;
  }
  private static int toDecimal(String s) {
    int total = 0;
    for (int i = 0; i < s.length(); i++) {
      char c = s.charAt(i);
      if (Character.isDigit(c))
        total += c - 48;
      else
        total += c - 55;
    }
    return total;
  }
}
